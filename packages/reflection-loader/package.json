{
  "name": "@getezy/grpc-reflection-loader",
  "version": "2.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --tsconfig tsconfig.build.json --format cjs,esm --dts --sourcemap --clean",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage"
  },
  "author": "Alexey Vasyukov (@notmedia)",
  "license": "MPL-2.0",
  "dependencies": {
    "grpc-js-reflection-client": "1.2.14"
  },
  "peerDependencies": {
    "@getezy/grpc-core": "*"
  },
  "devDependencies": {
    "@getezy/eslint-config-grpc": "*",
    "@vitest/coverage-v8": "1.6.0",
    "eslint": "9.5.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "prettier": "3.3.2",
    "tsup": "8.1.0",
    "typescript": "5.4.5",
    "typescript-eslint": "8.0.0-alpha.28",
    "vitest": "1.6.0"
  }
}
